{
  "manifest_version": 3,
  "name": "gen-intelligence",
  "version": "1.0.0",
  "description": "Save links to your Obsidian Knowledge Hub",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "host_permissions": [
    "https://*.ngrok-free.app/*",
    "http://localhost:8000/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "options_page": "options.html",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
